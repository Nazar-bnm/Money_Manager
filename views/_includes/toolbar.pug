header.mdc-toolbar.mdc-toolbar--fixed.demo-toolbar
  .mdc-toolbar__row
    section.mdc-toolbar__section.mdc-toolbar__section--align-start
      a.material-icons.mdc-toolbar__menu-icon(href='#') menu
      span.mdc-toolbar__title= title
    section.mdc-toolbar__section.mdc-toolbar__section--align-end(role='toolbar')
      a.material-icons.mdc-toolbar__icon(href='#', aria-label='Download', alt='Download') file_download
      a.material-icons.mdc-toolbar__icon(href='#', aria-label='Print this page', alt='Print this page') print
      a.material-icons.mdc-toolbar__icon(href='#', aria-label='Bookmark this page', alt='Bookmark this page') bookmark
